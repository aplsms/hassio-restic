{
    "name": "Restic",
    "version": "0.1",
    "slug": "restic",
    "description": "Create back-ups using restic",
    "arch": [
        "armhf",
        "armv7",
        "aarch64",
        "i386",
        "amd64"
    ],
    "startup": "once",
    "boot": "manual",
    "map": [
        "config",
        "ssl",
        "addons",
        "backup",
        "share",
        "media"
    ],
    "options": {
        "remote_url": ""
    },
    "schema": {
        "remote_url": "str"
    },
    "full_access":true,
    "privileged":[
        "NET_ADMIN", 
        "SYS_ADMIN", 
        "SYS_RAWIO", 
        "SYS_TIME", 
        "SYS_NICE", 
        "SYS_RESOURCE", 
        "SYS_PTRACE", 
        "SYS_MODULE", 
        "DAC_READ_SEARCH"
    ],
    "ports":{
        "22/tcp": 4848
    }
}
